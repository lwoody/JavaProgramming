package day_6_2;

public class ThreadTest2 {

	public static void main(String[] args) {

		SimpleThread a, b;
		a = new SimpleThread("one");
		b = new SimpleThread("two");
		
		Thread aa = new Thread(a);
		Thread bb = new Thread(b);

		aa.start();
		bb.start();

	}

}

