class SimpleThread implements Runnable {

	String name;

	public SimpleThread(String name) {
		this.name = name;
	}

	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.println(Thread.currentThread().getName() + "fdsafda");
		}
	}

}
